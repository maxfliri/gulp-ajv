{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": { "type": "number", "minimum": 1000 },
    "name": { "type": "string" },
    "email": { "type": "string", "format": "email"},
    "tags": { "type": "array", "items": { "type": "string" } }
  },
  "required": [ "id", "name", "email" ],
  "additionalProperties": false
}
